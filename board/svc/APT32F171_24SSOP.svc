        <Bit Name="LDRBS" Authority="RW" Bits="6" Description="LDRBS: 载入寄存器B中断&#xD;&#xA;0: 无效&#xD;&#xA;1: 清除LDRBS中断" />
        <Bit Name="LDRAS" Authority="RW" Bits="5" Description="LDRAS: 载入寄存器A中断&#xD;&#xA;0: 无效&#xD;&#xA;1: 清除LDRAS中断" />
        <Bit Name="CPCS" Authority="RW" Bits="4" Description="CPCS: 比较寄存器C匹配中断&#xD;&#xA;0: 无效&#xD;&#xA;1: 清除CPCS中断." />
        <Bit Name="CPBS" Authority="RW" Bits="3" Description="CPBS: 比较寄存器B匹配中断&#xD;&#xA;0: 无效&#xD;&#xA;1: 清除CPBS中断." />
        <Bit Name="CPAS" Authority="RW" Bits="2" Description="CPAS: 比较寄存器A匹配中断&#xD;&#xA;0: 无效&#xD;&#xA;1: 清除CPAS中断." />
        <Bit Name="LOVRS" Authority="RW" Bits="1" Description="LOVRS: 载入溢出中断&#xD;&#xA;0: 无效&#xD;&#xA;1: 清除LOVRS中断" />
        <Bit Name="COVFS" Authority="RW" Bits="0" Description="COVFS: 计数器溢出中断&#xD;&#xA;0: 无效&#xD;&#xA;1: 清除COVFS中断" />
